{
	"name": "sveltekit-starter-prismic-minimal",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"slicemachine": "start-slicemachine"
	},
	"dependencies": {
		"@fontsource-variable/urbanist": "^5.2.7",
		"@prismicio/client": "^7.21.6",
		"@prismicio/svelte": "^2.2.0",
		"@tailwindcss/vite": "^4.2.0",
		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"gsap": "^3.14.2",
		"tailwindcss": "^4.2.0",
		"three": "^0.183.0"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^10.0.1",
		"@iconify-json/fa-brands": "^1.2.2",
		"@iconify-json/ic": "^1.2.4",
		"@slicemachine/adapter-sveltekit": "^0.3.95",
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/kit": "^2.52.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.19",
		"@types/eslint": "^9.6.1",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"concurrently": "^9.2.1",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"slice-machine-ui": "^2.21.1",
		"svelte": "^5.53.0",
		"svelte-check": "^4.4.1",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"unplugin-icons": "^23.0.1",
		"vite": "^7.3.1"
	}
}
